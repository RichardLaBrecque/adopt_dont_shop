<h1><%= "#{@applicant.name}'s application page" %></h1>
<p><%= @applicant.name %></p>
<p><%= @applicant.street_address %></p>
<p><%= @applicant.city %></p>
<p><%= @applicant.zipcode %></p>
<p><%= @applicant.description %></p>
<<div class="search-pet">
  <h2>Search for a pet name</h2>
  <%= form_with url: "/applications/#{@applicant.id}?search=pet_name", method: :get, local: true do |f| %>
  <%= f.label :pet_name %>
  <%= f.text_field :pet_name %>
  <%= f.submit "Search" %>
  <% end %>
</div>
<% @applicant.pets.each do |pet| %>
<p>Applied for: <%= pet.name %></p>
<% end %>
<p><%= @applicant.status%></p>
